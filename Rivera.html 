$.cachedScript=function(e,c){return c=$.extend(c||{},{dataType:"script",cache:!0,url:e}),$.ajax(c)};

// Set a Cookie
function setCookie(name, value, days) {
  let expires = "";
  if (days) {
    const date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    expires = "; expires=" + date.toUTCString();
  }
  document.cookie = name + "=" + (value || "") + expires + "; path=/";
}
const getCookie = (name) => {
    const value = `; ${document.cookie}`;
    const parts = value.split(`; ${name}=`);
    if (parts.length === 2) return parts.pop().split(';').shift();
}
/** DARK MODE */
function toggleDarkMode(el){
	var darkIcon = '<i class="fa fa-moon-o"></i>';
	var lightIcon = '<i class="fa fa-sun-o" style="color:yellow"></i>';
	var elval = darkIcon;
	// var mode = getCookie('pps_dark_mode') ?? '1';
	if($('body').hasClass('dark_mode_ui')){
		setCookie('pps_dark_mode', '0', 365);
		$(el).html(darkIcon);
		// console.log('off', mode)
	}else{
		setCookie('pps_dark_mode', '1', 365);
		$(el).html(lightIcon);
		// console.log('on', mode)
	}
	$('body').toggleClass('dark_mode_ui');
	$('#main-logo').attr('src', $('body').hasClass('dark_mode_ui') ? base_url+'imgs/pps-logo-white.png' : base_url+'imgs/prepostseo.png?v=1');
}


