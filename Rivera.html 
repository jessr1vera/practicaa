<!-- search imove  -->
<script>
var dimensionY = 4;
var dimensionX = 2;
const searchContainer = document.querySelector('.search-results')
var selectedCell = "cell-0-0";
$('#inputSN, #searchInput-banner').keydown(function(event) {
    // console.log(event.target.id == 'inputSN');
    var SearchActive = $('.sbNew').hasClass('showB');
    if(SearchActive){
        dimensionY = ($('.search-row').length) || dimensionY;
                $(".match.selected").removeClass("selected");
        var currentX = parseInt(selectedCell.split("-")[1]);
        var currentY = parseInt(selectedCell.split("-")[2]);
        var c_href = '';
         switch (event.which) {
          case 37:
            selectedCell = "cell-" + (currentX == 0 ? dimensionX - 1 : currentX - 1) + "-" + currentY;
            break;
          case 38: // up
            selectedCell = "cell-" + currentX + "-" + (currentY == 0 ? dimensionY - 1 : currentY - 1);
                            if(currentY == 3)searchContainer.scrollTop = 0;
                        break;
          case 39:
            selectedCell = "cell-" + ((currentX + 1) % dimensionX) + "-" + currentY;
            break;
          case 40: // console.log('down')
            selectedCell = "cell-" + currentX + "-" + ((currentY + 1) % dimensionY);
            if(currentY == 3)  searchContainer.scrollTop = 200 ;
            else if(currentY == 7) searchContainer.scrollTop = 400;
            break;
        case 13:
            c_href = $("#" + selectedCell+">a").attr('href');
            if(c_href) window.location.href = c_href;
            break;
        }
        $("#" + selectedCell).addClass("selected");
        if(currentY == (dimensionY - 1) || $("#" + selectedCell).length == 0){
            searchContainer.scrollTop = 0; 
        }
    }
});

</script>







