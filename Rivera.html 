    docReady(function(){
		$(document).mouseup(function (e) {
			var container = $(".capDiv");
			if (( !container.is(e.target) && container.has(e.target).length === 0 ) || e.target.classList.value.indexOf('ppsbox-close') != -1) {
				$('.captcha-model').hide();
			}
		});
    });
</script>
<div id="floorad-wrapper"><span id="close-floorad-btn" onClick="this.parentElement.remove()">&#10006;</span>
    <div id="sticky-ad-background"></div><div id="adngin-footer_sticky-0"></div>
</div>

</body>
</html>